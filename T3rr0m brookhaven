local MenonerLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Advantord/Noner-Library/main/Library%20Noner')))()
local Window = MenonerLib:MakeWindow({Name = "BrookHaven", HidePremium = false, SaveConfig = true, ConfigFolder = "t3rr0m"})


-- Notification

MenonerLib:MakeNotification({
	Name = "Notification",
	Content = "Beta Test Script",
	Image = "",
	Time = 5
})


-- Tabs

local Main1000 = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://7733765224",
	PremiumOnly = false
})

local Home1000 = Window:MakeTab({
	Name = "Home",
	Icon = "rbxassetid://7733960981",
	PremiumOnly = false
})

local Car1000 = Window:MakeTab({
	Name = "Car",
	Icon = "rbxassetid://7733708835",
	PremiumOnly = false
})

local Tools1000 = Window:MakeTab({
	Name = "Tools",
	Icon = "rbxassetid://7734022041",
	PremiumOnly = false
	})
	
	local Visuals1000 = Window:MakeTab({
		Name = "Visuals",
		Icon = "rbxassetid://7743869411",
		PremiumOnly = false
		})

local Alsoscripts1000 = Window:MakeTab({
	Name = "Also Scripts",
	Icon = "rbxassetid://7743872365",
	PremiumOnly = false
	})
	

-- Section 


-- function Main

local Section = Main1000:AddSection({
	Name = "Player functions"
})

Main1000:AddTextbox({
	Name = "Walkspeed(Standart 16)",
	Default = "",
	TextDisappear = true,
	Callback = function(WS)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = WS
	end	  
})

Main1000:AddTextbox({
	Name = "Jump Height(Standart 50)",
	Default = "",
	TextDisappear = true,
	Callback = function(Jumpp)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Jumpp
	end	  
})

Main1000:AddTextbox({
	Name = "Gravity(Standart 192)",
	Default = "",
	TextDisappear = true,
	Callback = function(Gravitation)
		workspace.Gravity = Gravitation
	end	  
})

Main1000:AddTextbox({
	Name = "Fov(Standart 70)",
	Default = "",
	TextDisappear = true,
	Callback = function(Fov)
		workspace.CurrentCamera.FieldOfView = Fov
	end	  
})


-- function Home

local Section = Home1000:AddSection({
	Name = "Roomate Home"
	})

-- function Car

local Section = Car1000:AddSection({
		Name = "Car Speed"
		})
		
-- function Tools

local Section = Tools1000:AddSection({
	Name = "Server Tools"
	})
	
	
	
--	function Visuals
	
	local Section = Visuals1000:AddSection({
		Name = "Visuals Scripts Developers"
		})
		
		Visuals1000:AddButton({
	Name = "t3rr0m Esp",
	Callback = function()
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/Advantord/T3rr0m-Esp/main/T3rr0m%20Esp"),true))()
	end
	})
	
	Visuals1000:AddButton({
		Name = "t3rr0m Esp Line",
		Callback = function()
		loadstring(game:HttpGet(("https://raw.githubusercontent.com/Advantord/T3rr0m-Esp-Line/main/T3rr0m%20Esp%20Line"),true))()
		end
		})
		
		local Section = Visuals1000:AddSection({
		Name = "Players Tools"
		})
		
		Visuals1000:AddButton({
			Name = "Distributing Items From Playerss Inventories",
			Callback = function()
            for i,v in pairs (game.Players:GetChildren()) do

           for i,v in pairs(v.Backpack:GetChildren()) do
           local clone = v:Clone()
           clone.Parent = game.Players.LocalPlayer.Backpack
           end
            end
            end
			})
			
			Visuals1000:AddButton({
			Name = "Shows What Players Have In Their Inventory In The Console",
			Callback = function()
            for i,v in pairs (game.Players:GetChildren()) do

           for i,v in pairs(v.Backpack:GetChildren()) do
           print(v.Name)
           end
            end
            end
			})

